{
  "type": "module",
  "name": "fastify",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pm2:prod": "pm2 start ecosystem.config.js --env production --name=fastify-server",
    "pm2:gray": "pm2 start ecosystem.config.js --env gray --name=fastify-server",
    "pm2:test": "pm2 start ecosystem.config.js --env test --name=fastify-server",
    "pm2:dev": "pm2 start ecosystem.config.js --env dev --name=fastify-server",
    "pm2:stop": "pm2 stop server.js",
    "pm2:restart": "pm2 restart server.js",
    "pm2:delete": "pm2 delete server.js",
    "start:prod": "cross-env NODE_ENV=production NODE_LOCAL=default node server.js",
    "start:gray": "cross-env NODE_ENV=gray NODE_LOCAL=default node server.js",
    "start:test": "cross-env NODE_ENV=test NODE_LOCAL=default node server.js",
    "start:dev": "cross-env NODE_ENV=dev NODE_LOCAL=default node server.js",
    "start:nodemon": "cross-env NODE_ENV=dev NODE_LOCAL=default nodemon server.js"
  },
  "_moduleAliases": {
    "@module": "module",
    "@util": "util",
    "@plugins": "plugins",
    "@models": "models",
    "@common": "common",
    "@migrations": "migrations",
    "@prisma": "prisma",
    "@controllers": "controllers",
    "@routes": "routes",
    "@services": "services"
  },
  "keywords": [],
  "author": "five6",
  "license": "ISC",
